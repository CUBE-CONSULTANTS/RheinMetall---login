<mvc:View
	controllerName="login.login.controller.launch"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
<Page class="sapUiResponsivePadding--content sapUiResponsivePadding--header ">
  <headerContent>
    <Text class="sapUiTinyMarginBegin"
        text="Benvenuto TEST"
    />
    <ToolbarSpacer
    />
    <Title
        
        text="PORTALE FORNITORI"
        titleStyle="H4"
    />
    
  </headerContent>
  <content>
  <VBox class="sapUiLargeMarginTop"  width="100%" alignItems="Center" alignContent="Center" justifyContent="Center">
    <HBox justifyContent="SpaceAround" height="80%" width="100%" >
<GenericTile  class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Qualifica Fornitori"  press="onPress"
 >
<TileContent>
</TileContent>
</GenericTile>

<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Richieste Ordini d'Acquisto"
 press="press" >
<TileContent />
</GenericTile>

<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Lista Ordini d'Acquisto"
 press="press" >
<TileContent >
  
</TileContent>
</GenericTile>
</HBox>
</VBox>
</content>
<footer>
  <Toolbar>
    <Button text="Logout"
      type="Negative"
      icon="sap-icon://log"
      press="logOut" />
  </Toolbar>
  
</footer>
</Page>
</mvc:View>